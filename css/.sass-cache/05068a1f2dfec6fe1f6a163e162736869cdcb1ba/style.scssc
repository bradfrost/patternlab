/**
 * VARIABLES------------------------------Declarations of Sass variables 
 * -----Colors
 * -----Typography
 * -----Layout
 * -----Defaults
 * -----Breakpoints
 * MIXINS---------------------------------Sass mixins 
 * RESET----------------------------------Set reset defaults
 * GLOBAL ELEMENTS------------------------Establish global styles
 * -----Main
 * -----Headings
 * -----Text-related elements (p, blockquote, lists)
 * -----Defaults
 * -----Breakpoints
 * TYPOGRAPHY------------------------------
 * MEDIA------------------------------
 * LAYOUT------------------------------
 * NAVIGATION------------------------------
 * STYLE GUIDE INTERFACE------------------CSS for the Style Guide Container. Should not influence the containing styles at all.
 */
 
 
 
 
 
/*------------------------------------*\
    $VARIABLES
\*------------------------------------*/

/* Colors */
$primary : #000;
$secondary : #808080;
$tertiary : #ddd;
$quaternary : #eee;
$quinary : #f7f7f7;
$error : #f00;
$valid : #00a651;
$white : #fff;
$black : #000;

/* Typography */
$font : "HelveticaNeue", "Helvetica", "Arial", sans-serif;
$font-secondary : "Georgia, "Times New Roman", Times, serif";
$font-size-norm : 100%;
$font-size-sm : 70%;
$font-size-large : 120%;

/* Layout */
$max-width: 64em;

/* Defaults */
$space : 1em;
$doublespace : $space*2;
$halfspace : $space/2;
$quarterspace : $space/4;
$pad : 1em;
$halfpad : $pad/2;
$quarterpad :$pad/4;


/* Breakpoints */
$bp-small : 24em;
$bp-med : 44em;
$bp-large : 60em;
$bp-xl : 80em;






/*------------------------------------*\
    $MIXINS
\*------------------------------------*/
@mixin transition($transition-property, $transition-time, $method) {
	-webkit-transition: $transition-property $transition-time $method;
	-moz-transition: $transition-property $transition-time $method;
	-ms-transition: $transition-property $transition-time $method;
	-o-transition: $transition-property $transition-time $method;
	transition: $transition-property $transition-time $method;
}






/*------------------------------------*\
    $RESET
\*------------------------------------*/
* { 
	-moz-box-sizing: border-box; 
	-webkit-box-sizing: border-box; 
	box-sizing: border-box; 
}
html, body, div, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, ol, ul, li, form, legend, label, table, header, footer, nav, section, figure { 
	margin: 0; 
	padding: 0; 
} 
table { 
	border-collapse: collapse; 
	border-spacing: 0; 
}
header, footer, nav, section, article, hgroup, figure {
	display: block;	
}






/*------------------------------------*\
    $GLOBAL CLASSES
\*------------------------------------*/
/**
 * Completely remove from the flow and screen readers.
 */

.is-hidden {
    display: none !important;
    visibility: hidden !important;
}

/**
 * Completely remove from the flow but leave available to screen readers.
 */

.is-vishidden {
    position: absolute !important;
    overflow: hidden;
    width: 1px;
    height: 1px;
    padding: 0;
    border: 0;
    clip: rect(1px, 1px, 1px, 1px);
}






/*------------------------------------*\
    $GLOBAL ELEMENTS
\*------------------------------------*/
body {
	background: $white;
	font: 100%/1.4 $font;
}

/* Headings */
/*Further Reading: http://csswizardry.com/2012/02/pragmatic-practical-font-sizing-in-css/ */
h1, .alpha   { 
	margin: 0 0 $quarterspace;
}

h2, .beta    { 
	margin: 0 0 $quarterspace;
}

h3, .gamma   { 
	margin: 0 0 $quarterspace;
}

h4, .delta   {
	margin: 0 0 $quarterspace;
}
h5, .epsilon { 
	margin: 0 0 $quarterspace;
}
h6, .zeta    { 
	margin: 0 0 $quarterspace;
}
hgroup {
	margin-bottom: $space;
}
hgroup h1, hgroup h2, hgroup h3 {
	margin: 0;
}


/* Text-Related Elements */ 
p {
	margin-bottom: $space;
}

ol, ul {
	margin: 0 0 $space 1.2em;
}

blockquote {
	font: italic $font-size-large $font-secondary;
	border-left: 0.25em solid $tertiary;
	padding-left: $pad;
}

/* Horizontal Rule */ 
hr {
	border: 0;
    height: 1px;
    background: $secondary;
    margin: $doublespace 0;
}


/* Links */ 
a {
	color: $secondary;
	text-decoration: none;
	outline: 0;
	&:hover, &:focus {
		color: $primary;
	}
}

/* Media */
img, video, object {
	max-width: 100%;
	height: auto;
	margin-bottom: $space;
}
iframe {
	margin-bottom: $space;
}

figure {
	margin-bottom: $space;
	img {
		margin-bottom: $quarterspace;
	}
}
figcaption {
	font-style: italic;
}






/*------------------------------------*\
    $LAYOUT
\*------------------------------------*/

[role=main].clean { /* For 'Clean' mode, which hides all style guide chrome leaving only the content. */
	max-width: $max-width;
	margin: 0 auto;
	overflow: hidden;
}

/* Grid */
.g {
	overflow: hidden;
	margin: 0 0 $quarterspace;
}
[class*='c-'] {
	padding: $quarterspace $space;
}
@media all and (min-width: $bp-med) {
	[class*='c-'] {
		float: left;
	}
	.c-1-1 {
		width: 100%;
	}
	.c-1-2 {
		width: 50%;
	}
	.c-1-3 {
		width: 33.333333%;
	}
	.c-2-3 {
		width: 66.666666%;
	}
	.c-1-4 {
		width: 25%;
	}
	.c-3-4 {
		width: 75%;
	}
	.c-1-5 {
		width: 20%;
	}
	.c-2-5 {
		width: 40%;
	}
	.c-3-5 {
		width: 60%;
	}
	.c-4-5 {
		width: 80%;
	}
	.c-1-6 {
		width: 16.66666666%;
	}
	.c-5-6 {
		width: 83.333333333%;
	}
}
.equal-width {
	@media all and (min-width: $bp-med) {
		.c-1-4 {
			width: 50%;
			&:nth-of-type(2n+1) {
				clear: both;
			}
		}
		.c-1-5, .c-1-6 {
			width: 33%;
			&:nth-of-type(3n+1) {
				clear: left;
			}
		}
	}
	
	@media all and (min-width: $bp-large) {
		.c-1-4 {
			width: 25%;
			&:nth-of-type(2n+1) {
				clear: none;
			}
		}
		.c-1-5 {
			width: 20%;
			&:nth-of-type(3n+1) {
				clear: none;
			}
		}
		.c-1-6 {
			width: 16.66666666%;
			&:nth-of-type(3n+1) {
				clear: none;
			}
		}
	}
	
}

/* Demo */
.demo [class*='c-'] {
	background: $tertiary;
	color: $secondary;
	text-align: center;
	
	&:nth-of-type(2n) {
		background: $quaternary;
	}
}





/*------------------------------------*\
    $NAVIGATION
\*------------------------------------*/

/* Primary Navigation */
.p-nav {
	overflow: hidden;
}
.nav-toggle {
	float: right;
	display: block;
	padding: $pad;
	@media all and (min-width: $bp-med) {
		display: none;
	}
}
.nav {
	clear: both;
	list-style: none;
	margin: 0 0 $space;
	border-top: 1px solid $secondary;
	li {
		border-bottom: 1px solid $secondary;
		a {
			display: block;
			padding: $pad;
			&:hover, &:focus {
				background: $tertiary;
			}
		}
		@media all and (min-width: $bp-med) {
			display: inline-block;
			margin-right: 1em;
			border: 0;
		}
	}
}

/* Pagination */
.pagination {
	list-style: none;	
	li {
		display: inline-block;
		margin: 0 0.2em;
		a:link, a:visited {
			border: 1px solid $tertiary;
			padding: 0.5em;
		}
	}
	 
}

/* Breadcrumbs */
.crumbs li {
	display: inline-block;
	margin-right: 0.2em;
	&:after {
		content: "â†’";
		padding-left: 0.5em;
	}
	&:last-child:after {
		content: "";
	}
}




/*------------------------------------*\
    $FORMS
\*------------------------------------*/
form {
	margin-bottom: $space;
}
form div {
	margin-bottom: $space;
}
form ol, form ul {
	list-style: none;
	margin-left: 0;
}
legend {
	display: none;
}
fieldset {
	border: 0;
	padding: 0;
	margin: 0;
}
label {
	display: block;
	padding-bottom: $quarterspace;
}
button, input, select, textarea {
    font-family: inherit;
    font-size: 100%;
    margin: 0;
}
input, textarea {
	width: 100%;
	border: 1px solid $secondary;
	padding: $halfpad;
}
input[type="checkbox"],
input[type="radio"] {
    width: auto;
    margin-right: 0.3em;
}
input[type="search"] {
    -webkit-appearance: textfield; 
}
input[type="search"]::-webkit-search-cancel-button,
input[type="search"]::-webkit-search-decoration {
    -webkit-appearance: none;
}

.inline-form {
	position: relative;
	input[type=submit] {
		position: absolute;
		top: 0;
		right: 0;
		z-index: 1;
		width: auto;
	}
}

.left-form {
	@media all and (min-width: $bp-med) {
		div {
			overflow: hidden;
		}
		label {
			float: left; 
			width: 15%;
			padding: $halfpad $pad 0 0;
			text-align: right;
		}
		input, textarea, select {
			float: left;
			width: 85%;
		}
	}
}
.has-error {
	border-color: $error;
}
.is-valid {
	border-color: $valid;
}
.error {
	display: block;
	color: $error;
}
.valid {
	display: block;
	color: $valid;
}




/*------------------------------------*\
    $BUTTONS
\*------------------------------------*/
.btn {
	display: inline-block;
	background: $secondary;
	color: $white;
	padding: $halfpad;
	border: 0;
	&:hover {
		background: $tertiary;
		color: $white;
	}
}
.btn-sm {
	font-size: $font-size-sm;
}
.btn-lg {
	font-size: $font-size-large;
	padding: $pad;
}
.btn-alt {
	background: $tertiary;
	&:hover {
		background: $secondary;
	}
}

/*------------------------------------*\
    $Table
\*------------------------------------*/
table {
	width: 100%;
}
th {
	text-align: left;
}
